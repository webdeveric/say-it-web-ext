{
  "name": "@webdeveric/say-it-web-ext",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "description": "Say it",
  "type": "module",
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "WebExtension",
    "web-extension",
    "speechSynthesis"
  ],
  "author": {
    "email": "eric@webdeveric.com",
    "name": "Eric King",
    "url": "http://webdeveric.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdeveric/say-it-web-ext.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveric/say-it-web-ext/issues"
  },
  "homepage": "https://github.com/webdeveric/say-it-web-ext",
  "scripts": {
    "clean": "rimraf ./dist/ ./build/",
    "lint": "eslint ./*.js ./*.cjs ./src --ext .ts --ext .ts,.tsx,.js,.jsx",
    "spellcheck": "cspell --no-progress './**/*'",
    "format": "prettier --write .",
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack --watch",
    "presign": "npm run build",
    "sign": "web-ext sign --config web-ext-config.cjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "version": "sync-package-version ./src/manifest.json | xargs git add",
    "prepare": "husky install"
  },
  "prettier": "@webdeveric/prettier-config",
  "browserslist": [
    "extends @webdeveric/browserslist-config/web-extensions"
  ],
  "dependencies": {
    "@types/react-dom": "^18.2.13",
    "@types/react": "^18.2.28",
    "@types/webextension-polyfill": "^0.10.4",
    "@webdeveric/utils": "^0.27.1",
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.93",
    "@vitest/coverage-v8": "^0.34.6",
    "@webdeveric/browserslist-config": "^0.2.0",
    "@webdeveric/eslint-config-ts": "^0.7.1",
    "@webdeveric/prettier-config": "^0.2.0",
    "@webdeveric/sync-package-version": "^0.4.2",
    "autoprefixer": "^10.4.16",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.33.0",
    "cspell": "^7.3.8",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint": "^8.51.0",
    "happy-dom": "^12.9.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react-dom": "^18.2.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.5.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "web-ext-plugin": "^2.8.1",
    "web-ext": "^7.8.0",
    "webextension-polyfill": "^0.10.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^5.1.4",
    "webpack": "^5.89.0"
  }
}
